    if getgenv().AutoUiMuiGod == true then
if Humanoid.Health <= Humanoid.MaxHealth * 0.15 then
Plr.Backpack.ServerTraits.Use:InvokeServer(Char.Ki.Value);
wait()
Plr.Backpack.ServerTraits.Transform:FireServer("g")
end
end
end)
-- Auto Ui/Mui/God



-- Auto Block
AutoBlock = {KeyAutoBlock}

Blocking = false;
game:GetService('RunService').RenderStepped:connect(function()
if Blocking then
Plr.Backpack.ServerTraits.Input:FireServer({[1] = "blockon"}, CFrame.new(0, 0, 0), nil)
for i, v in pairs(Char:GetChildren()) do
   if v.Name == "Block" and v.Value == true then
   v.Value = false
end;
end;
end;
end)

Mouse.KeyDown:connect(function(Key)
if Key == AutoBlock[1] then
Blocking = not Blocking;
    Plr.Backpack.ServerTraits.Input:FireServer({[1] = "blockoff"}, CFrame.new(0, 0, 0), nil)
end;
end)
-- Auto Block
else
game:shutdown();
end
else
game:shutdown();
end
