local Char = Plr.Character
local Humanoid = game.Players.LocalPlayer.Character.Humanoid
local OldHealth = Humanoid.Health
local Mouse = Plr:GetMouse()
-- Variables



-- Dodge Mode
Mouse.KeyDown:connect(function(Keybind)
Keybind = Keybind:lower()
if Keybind == KeyDodgeMode  then
if getgenv().DodgeMode == true then
Char.HumanoidRootPart.Touched:Connect(function()
   wait(.40)
   Char.HumanoidRootPart.CFrame = Char.HumanoidRootPart.CFrame * CFrame.new(0,0,0.5)
end)

game.StarterGui:SetCore("SendNotification", {
Title = "DODGE MODE ON"; -- the title (ofc)
Text = "Keybind = "..KeyDodgeMode; -- what the text says (ofc)
Duration = 5; -- how long the notification should in secounds
})
end
end
