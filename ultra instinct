-- Random Place Tp
local LocalPlayer = game.Players.LocalPlayer
local Character = LocalPlayer.character
getgenv().ON = false;
Mouse.KeyDown:Connect(function(Keybind)
    if Keybind == KeyOnRandomPlaceTp then
        getgenv().ON = true
        while getgenv().ON do
            wait()
            Char.HumanoidRootPart.CFrame = Char.HumanoidRootPart.CFrame * CFrame.new(6, 0, 0)
            wait(.1)
            Char.HumanoidRootPart.CFrame = Char.HumanoidRootPart.CFrame * CFrame.new(-4,0,0)
            wait(.54)
            Char.HumanoidRootPart.CFrame = Char.HumanoidRootPart.CFrame * CFrame.new(9,0,0)
            wait(.54)
            Char.HumanoidRootPart.CFrame = Char.HumanoidRootPart.CFrame * CFrame.new(-2,0,0)
            wait(.1)
            Char.HumanoidRootPart.CFrame = Char.HumanoidRootPart.CFrame * CFrame.new(17,0,0)
            end
        end
    end)
Mouse.KeyDown:Connect(function(Keybind)
    if Keybind == KeyOffRandomPlaceTp then
        getgenv().ON = false
    end
end)
-- Random Place Tp



-- LeftCtrl + Click TP Punch
if getgenv().ClickTpPunch == true then
Mouse.Button1Down:connect(function()
if not game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.LeftControl) then return end
if not Mouse.Target then return end

local function GetClosestPlayer()
   local target = nil
   local distance = math.huge
 
   for i,v in next, Players:GetPlayers() do
       if v and v ~= Plr and v.Character and v.Character:FindFirstChildOfClass('Humanoid') and v.Character:FindFirstChildOfClass('Humanoid').RootPart then
		   Plr.Backpack.ServerTraits.Vanish:FireServer()
           local plrdist = Plr:DistanceFromCharacter(v.Character:FindFirstChildOfClass('Humanoid').RootPart.CFrame.p)
           if plrdist < distance then
               target = v
               distance = plrdist
           end
       end
   end
   return target
end

Char:FindFirstChildOfClass("Humanoid").RootPart.CFrame = GetClosestPlayer().Character:FindFirstChildOfClass('Humanoid').RootPart.CFrame
end)
end
-- LeftCtrl + Click TP Punch



-- Ultra Instinct Effects
if getgenv().FakeUltraInstinct == true then
    Char.HumanoidRootPart["UltraTrans"]:Play()
    local Form = Instance.new("Folder")
    Form.Name = "ui"
    Form.Parent = Char
    else Char["ui"]:remove()
end
-- Ultra Instinct Effects



-- Auto Ui/Mui/God
